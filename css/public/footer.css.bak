
#footer{
    position: relative;
    background-color: rgb(245,245,245);
    margin-bottom: 0;
    margin-top: 10px;
}
#footer .container .col-md-3 .title{
    margin: 30px 0 20px;
    font-weight: bold;
    font-size: 20px;
    color: rgb(51,51,51);
}
#footer .container .col-md-3 ul{
    list-style: none;
    padding-left:0;
}
#footer .container .col-md-3 li{
    height: 32px;
}
#footer .container .col-md-3 li a{
    text-decoration: none;
    color: rgb(121,121,121);

}

.footer-bottom{
    height: 120px;
    line-height: 60px;
    color: white;
    font-size: 16px;
    background-color: rgb(6,109,179);
}
.footer-bottom .bottom-warn{
    line-height: 22px;
    font-size: 14px;
}

.copyright p{
    line-height: 30px;
    margin-bottom: 0;
    font-size: 14px;
}

.footer-bottom .share-icon{
    height: 60px;
}
.footer-bottom .share-icon ul{
    list-style: none;
    height: 60px;
   /*margin: 0;*/
}
.footer-bottom .share-icon li{
    float: left;
    width: 35px;
    height: 35px;

    background-color: white;
    margin: 12.5px 10px;
    border-radius: 50%;
}
.footer-bottom .share-icon img{
    font-size: 0;
    width: 15px;
    height: 15px;
    margin: 10px;
    display: block;
}
.footer-bottom .share-icon li:nth-child(3) img{
    font-size: 0;
    width: 9px;
    height: 17px;
    margin: 9px 13px;
    display: block;
}